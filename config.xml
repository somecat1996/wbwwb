<?xml version='1.0' encoding='utf-8'?>
<widget id="com.project.wbwwb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WeBecomeWhatWeBehold</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0x00000000" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="Orientation" value="landscape" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="hdpi" src="res/icons/android/land-hdpi.png" />
        <icon density="ldpi" src="res/icons/android/land-ldpi.png" />
        <icon density="mdpi" src="res/icons/android/land-mdpi.png" />
        <icon density="xhdpi" src="res/icons/android/land-xhdpi.png" />
        <icon density="xxhdpi" src="res/icons/android/land-xxhdpi.png" />
        <icon density="xxxhdpi" src="res/icons/android/land-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icons/ios/land-@3x.png" width="180" />
        <icon height="60" src="res/icons/ios/land-.png" width="60" />
        <icon height="120" src="res/icons/ios/land-@2x.png" width="120" />
        <icon height="76" src="res/icons/ios/land-.png" width="76" />
        <icon height="152" src="res/icons/ios/land-@2x.png" width="152" />
        <icon height="40" src="res/icons/ios/land-.png" width="40" />
        <icon height="80" src="res/icons/ios/land-@2x.png" width="80" />
        <icon height="57" src="res/icons/ios/land-.png" width="57" />
        <icon height="114" src="res/icons/ios/land-@2x.png" width="114" />
        <icon height="72" src="res/icons/ios/land-.png" width="72" />
        <icon height="144" src="res/icons/ios/land-@2x.png" width="144" />
        <icon height="29" src="res/icons/ios/land-.png" width="29" />
        <icon height="58" src="res/icons/ios/land-@2x.png" width="58" />
        <icon height="50" src="res/icons/ios/land-.png" width="50" />
        <icon height="100" src="res/icons/ios/land-@2x.png" width="100" />
    </platform>
    <engine name="browser" spec="^5.0.3" />
    <engine name="ios" spec="^4.5.4" />
</widget>
