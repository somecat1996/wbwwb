<?xml version='1.0' encoding='utf-8'?>
<widget id="com.project.wbwwb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <name>WeBecomeWhatWeBehold</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="hdpi" src="res/icon/android/land-hdpi.png" />
    <icon density="ldpi" src="res/icon/android/land-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/land-mdpi.png" />
    <icon density="xhdpi" src="res/icon/android/land-xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/land-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icon/android/land-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0x00000000" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
